<div class="p-5 mb-4 bg-light text-dark border rounded-3" id="<%= authFlow.name %>">
    <div class="card">
        <%- include('flowheader', {
            flow: authFlow.flow,
            variant: authFlow.variant,
            title: authFlow.title, 
            image: authFlow.image, 
            protocol: authFlow.protocol,
            description: authFlow.description,
            user_input: authFlow.configuration.user_input
        }) %> 
        <%- include('flowbody', {
            name: authFlow.name,
            config: authFlow.configuration
        }) %>
    </div>
</div>
