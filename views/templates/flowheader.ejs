<div class="card-body">
    <h2><%= title %> (<%= protocol %>)</h2>
    <p><%= description %></p>
    <figure>
        <img
            class="card-img-bottom"
            src=<%= image %>
        />
        <figcaption><%= title %> diagram - created by Lawrence Newcombe</figcaption>
    </figure>
    <% if(user_input) { %>
        <%- include('userinput/' + user_input.template) %>
    <% } %>
    <button id="deviceProduction" role="button" class="btn btn-primary btn-lg" onclick="launchFlow('<%= name %>', false)">
        Production
    </button>
    <button id="deviceSandbox" role="button" class="btn btn-primary btn-lg" onclick="launchFlow('device', true)">
        Sandbox
    </button>
</div>
